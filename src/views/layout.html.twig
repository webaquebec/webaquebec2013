<!doctype html>
<!--[if lt IE 7]> <html class="no-js lt-ie9 lt-ie8 lt-ie7" lang="fr"> <![endif]-->
<!--[if IE 7]>    <html class="no-js lt-ie9 lt-ie8" lang="fr"> <![endif]-->
<!--[if IE 8]>    <html class="no-js lt-ie9" lang="fr"> <![endif]-->
<!--[if gt IE 8]><!--> <html class="no-js" lang="fr"> <!--<![endif]-->
<head>
    <meta charset="utf-8">

    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">

    <title>{% block title %}{% endblock %}Web à Québec</title>
    <meta name="description" content="Le Web À Québec c'est trois jours de rencontres par et pour les gens qui imaginent le web.">

    <meta name="viewport" content="width=device-width">

    <link rel="stylesheet" href="/assets/stylesheets/global.css">
    {% block stylesheets %}{% endblock %}
      <!--[if lt IE 9]>
      <script src="//html5shiv.googlecode.com/svn/trunk/html5.js"></script>
      <![endif]-->
</head>
<body class="layout{% block bodyClasses %}{% endblock %}" itemscope itemtype="http://schema.org/Event">
    {% block body %}{% endblock %}
    
    <footer id="nous-joindre" data-section>
        <span class="gradient"></span>
        <span class="diag"></span>
        <div class="address">
            
            <span class="block" itemprop="location" itemscope itemtype="http://schema.org/PostalAddress">
                <h3>Adresse de l'événement</h3>
                <span itemprop="streetAddress">100 rue Quai St-André</span>
                <span itemprop="addressLocality">Québec (Qc)</span>
                <span itemprop="postalCode">G1K 3Y2</span>
            </span>
            
            <span class="block" itemtype="http://schema.org/PostalAddress">
                <h3>Siège social</h3>
                <span itemprop="streetAddress">305 Charest Est, bureau 901</span>
                <span itemprop="addressLocality">Québec (Qc)</span>
                <span itemprop="postalCode">G1K 3H3</span>
                <span itemprop="telephone">418 647-3877</span>
            </span>
            
        </div>
        <form action="http://webaquebec.createsend.com/t/r/s/jjjdthl" method="post" id="newsletter">
            <label>
                <span class="label">Inscrivez-vous à l’infolettre du WAQ !</span>
                <input type="email" name="cm-jjjdthl-jjjdthl" id="some_name" >
            </label>
            <button type='submit'><span class="shy">Envoyer</span></button>
        </form>
        <span id="ix">Conception: <a href="http://ixmedia.com">iXmédia</a></span>
    </footer>
    
    <script type="text/javascript" src="http://s7.addthis.com/js/300/addthis_widget.js#pubid=ra-505c8d037002272c"></script>
    <script src="//ajax.googleapis.com/ajax/libs/jquery/1.8.2/jquery.min.js"></script>
    <script>window.jQuery || document.write('<script src="/assets/javascripts/libs/jquery.js"><\/script>')</script>
    {% block javascripts %}{% endblock %}
</body>
</html>
